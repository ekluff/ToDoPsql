<div class="row text-center">
  <h3>Lists</h3>


<table class="table table-hover">
  <tr id="first-row">
    <th width="35%">Name</th>
    <th width="35%">Category</th>
    <td width="20%" >
      <a href="/list/new" class="btn btn-primary btn-sm">+ List</a>
    </td>
  </tr>
  <% @lists.each do |list| %>
    <tr><td width="100%"><a href="/list/<%= list.id %>"><%= list.name %></a></td></tr>
  <% end %>
</table>

</div>
